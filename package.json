    "autoprefixer": "^9.8.6",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.12",
    "postcss": "^7.0.36",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.4",
    "tailwindcss-nested-groups": "^1.2.4"
